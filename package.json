{
  "name": "tiny-warning",
  "version": "1.3.1",
  "description": "A tiny warning function",
  "author": "Alex Reardon <alexreardon@gmail.com>",
  "license": "MIT",
  "keywords": [
    "warning",
    "error",
    "assert",
    "asserts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alexreardon/tiny-warning.git"
  },
  "bugs": {
    "url": "https://github.com/alexreardon/tiny-warning/issues"
  },
  "main": "dist/tiny-warning.cjs.js",
  "module": "dist/tiny-warning.esm.js",
  "types": "dist/tiny-warning.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/tiny-warning.js",
      "default": {
        "types": "./dist/tiny-warning.d.ts",
        "default": "./dist/tiny-warning.cjs.js"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "/dist",
    "/src"
  ],
  "size-limit": [
    {
      "path": "dist/tiny-warning.min.js",
      "limit": "190B"
    },
    {
      "path": "dist/tiny-warning.js",
      "limit": "260B"
    },
    {
      "path": "dist/tiny-warning.cjs.js",
      "limit": "169B"
    },
    {
      "path": "dist/tiny-warning.esm.js",
      "import": "foo",
      "limit": "106B"
    }
  ],
  "scripts": {
    "test": "yarn jest",
    "test:size": "yarn build && yarn size-limit",
    "prettier:write": "yarn prettier --debug-check src/** test/**",
    "prettier:check": "yarn prettier --write src/** test/**",
    "typescript:check": "yarn tsc --noEmit src/*.ts test/*.ts",
    "validate": "yarn prettier:check && yarn typescript:check",
    "build:clean": "rimraf dist",
    "build:flow": "cp src/tiny-warning.js.flow dist/tiny-warning.cjs.js.flow",
    "build:typescript": "tsc ./src/tiny-warning.ts --emitDeclarationOnly --declaration --outDir ./dist",
    "build:typescript:esm": "tsc ./src/tiny-warning.ts --emitDeclarationOnly --declaration --outDir ./dist/esm",
    "build:dist": "yarn rollup --config rollup.config.js",
    "build": "yarn build:clean && yarn build:dist && yarn build:typescript && yarn build:typescript:esm",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/jest": "^29.1.1",
    "@types/rollup": "^0.54.0",
    "expect-type": "^0.14.2",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "size-limit": "^8.1.0",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "packageManager": "yarn@1.22.19"
}
